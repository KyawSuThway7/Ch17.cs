using System;

namespace Ch17Inheritance
{
    public class Mammal
    {
        public string name;
        public virtual void Eating()
        {
            Console.WriteLine("food : Eating Something");
        }
        public virtual void Breathing()
        {
            Console.WriteLine("Breath : Always Breathing");
        }
        public virtual void Sleeping()
        {
            Console.WriteLine("Sleep : Sleeping sometimes");
        }
        public void showinf()
        {
            Console.WriteLine($"Name : {name}");
        }
        
    }
}
